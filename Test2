SELECT
    fabid,
    jobid,
    jobver,
    LISTAGG(chart_rule, ',') WITHIN GROUP (ORDER BY chart_rule) AS combined_column
FROM (
    SELECT DISTINCT
        fabid,
        jobid,
        jobver,
        chart_type || ',' || ruleid AS chart_rule
    FROM
        rule_action
)
GROUP BY
    fabid,
    jobid,
    jobver;

SELECT t.*
FROM your_table t
WHERE EXISTS (
    SELECT 1
    FROM (
        -- å¾Œæ®µå¤§æ‹¬è™Ÿè®Šæ•¸ v1;v2;v100...
        SELECT REGEXP_SUBSTR(
                   LOWER(REPLACE(REGEXP_SUBSTR(t.column_data, '\{([^}]*)\}$', 1, 1, NULL, 1)), ' ', ''),
                   'v\d+', 1, LEVEL
               ) AS v_var
        FROM dual
        CONNECT BY REGEXP_SUBSTR(
                       LOWER(REPLACE(REGEXP_SUBSTR(t.column_data, '\{([^}]*)\}$', 1, 1, NULL, 1)), ' ', ''),
                       'v\d+', 1, LEVEL
                   ) IS NOT NULL
    ) back_vars
    WHERE back_vars.v_var IS NOT NULL
      AND back_vars.v_var NOT IN (
          -- å‰æ®µå®šç¾©éçš„ v åç¨±
          SELECT REGEXP_SUBSTR(
                     LOWER(REPLACE(REGEXP_SUBSTR(t.column_data, '^\{([^}]*)\}', 1, 1, NULL, 1)), ' ', ''),
                     'v\d+', 1, LEVEL
                 )
          FROM dual
          CONNECT BY REGEXP_SUBSTR(
                         LOWER(REPLACE(REGEXP_SUBSTR(t.column_data, '^\{([^}]*)\}', 1, 1, NULL, 1)), ' ', ''),
                         'v\d+', 1, LEVEL
                     ) IS NOT NULL
      )
);

